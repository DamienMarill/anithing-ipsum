{
  "name": "anything-ipsum",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "start:prod": "npm run build && npm run serve:ssr:anything-ipsum",
    "build": "nx build",
    "build:prod": "nx build --configuration production",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "serve:ssr:anything-ipsum": "node dist/anything-ipsum/server/server.mjs",
    "dev": "nx serve --open",
    "lint": "nx lint",
    "build:figma": "cd figma-plugin && node build-plugin.js",
    "build:figma:dev": "cd figma-plugin && NODE_ENV=development node build-plugin.js"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.3",
    "@tailwindcss/postcss": "^4.1.11",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^20.1.3",
    "@angular-devkit/schematics": "^20.1.3",
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.0",
    "@eslint/js": "^9.8.0",
    "@figma/plugin-typings": "^1.116.0",
    "@nx/angular": "21.3.11",
    "@nx/esbuild": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@schematics/angular": "^20.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jasmine-core": "~5.8.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nx": "21.3.11",
